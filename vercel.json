{
    "version": 2,
    "builds": [
        {
            "src": "/public/index.php",
            "use": "vercel-php@0.6.1"
        }
    ],
    "routes": [
        {
            "src": "/(css|js|images|fonts|svg|ico|webfonts)(.*)",
            "dest": "/public/$1$2"
        },
        {
            "src": "/(.*)",
            "dest": "/public/index.php"
        }
    ]
}
